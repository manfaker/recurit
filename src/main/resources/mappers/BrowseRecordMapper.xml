<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shenzhen.recurit.dao.BrowseRecordMapper">

        <resultMap id="baseResultMap" type="com.shenzhen.recurit.vo.BrowseRecordVO">
            <id column="id" property="id"/>
            <result column="position_id" property="positionId"/>
            <result column="create_date" property="createDate"/>
            <result column="creater" property="createDate"/>
            <result column="company_code" property="companyCode"/>
            <result column="user_code" property="userCode"/>
        </resultMap>

        <sql id="baseResult">
            position_id,create_date,creater,company_code,user_code
        </sql>

        <insert id="saveBrowseRecord" keyProperty="id" keyColumn="id" useGeneratedKeys="true">
            insert into tab_browse_record(<include refid="baseResult"/>)
                values(#{positionId},#{createDate},#{createDate},#{companyCode},#{userCode});
        </insert>


</mapper>
